    <td><a style="font-size:14px;" target="_blank" href="/uploads/monetique/chargements/{{ historique.chargement }}"> {{ historique.chargement }}</a></td>
    <td>
        {% if historique.extraction %}
            <a style="font-size:14px;" target="_blank" href="/uploads/monetique/extractions/{{ historique.extraction }}"> {{ historique.extraction }}</a>
        {% else %}
            <a style="font-size:14px;" href="#" class="btn btn-outline-dark">Extraire</a>
        {% endif %}
    </td>
</tr>























